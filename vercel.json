{
    "version": 2,
    "env": {
      "VERCEL": "true"
    },
    "routes": [
      {
        "src": "/api/lists",
        "dest": "/api/lists.js"
      },
      {
        "src": "/api/subscribers",
        "dest": "/api/subscribers.js"
      },
      {
        "src": "/",
        "dest": "/public/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }